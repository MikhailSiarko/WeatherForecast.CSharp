{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ForecastUrl": "http://api.openweathermap.org/data/2.5/forecast?q={0}&units=metric&appid={1}",
  "JwtOptions": {
    "Key": "AuthenticationKey",
    "Issuer": "ParcelTracker_API",
    "Audience": "ParcelTracker_Client",
    "Lifetime": 30
  },
  "ExpirationTime": 20,
  "ConnectionStrings": {
    "Default": "Data Source=../Database.db"
  }
}
