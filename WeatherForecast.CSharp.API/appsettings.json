{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "WeatherAPI": {
    "BaseUrl": "http://api.openweathermap.org/",
    "ForecastUrlFormat": "data/2.5/forecast?q={0}&units=metric&appid={1}",
    "IconUrlFormat": "http://openweathermap.org/img/wn/{0}@2x.png"
  },
  "JwtOptions": {
    "Key": "AuthenticationKey",
    "Issuer": "ParcelTracker_API",
    "Audience": "ParcelTracker_Client",
    "Lifetime": 30
  },
  "ExpirationTime": 20,
  "ConnectionStrings": {
    "Default": "Data Source=../Database.db"
  }
}
